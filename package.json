{
  "name": "fashion-world-v3",
  "version": "3.2.2",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --include-path ./src --watch --recursive",
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "node scripts/build.js",
    "build": "npm-run-all build-css build-js",
    "test": "babel-node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "ant-design-pro": "^0.3.1",
    "antd": "^3.0.0",
    "autoprefixer": "^7.2.3",
    "axios": "^0.17.1",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.3.0",
    "clark": "^0.0.6",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.8",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.41.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.6",
    "filesize": "^3.5.11",
    "fs-extra": "3.0.1",
    "github-markdown-css": "^2.9.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "import-local": "^1.0.0",
    "jest": "^22.0.5",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^2.0.10",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.0",
    "raf": "3.4.0",
    "rc-animate": "^2.4.1",
    "rc-queue-anim": "^1.4.0",
    "react": "^16.1.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dev-utils": "^4.2.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.1.0",
    "react-highlight-words": "^0.10.0",
    "react-markdown": "^3.1.1",
    "react-player": "^1.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "tiny-qiniu": "^0.5.0",
    "url-loader": "0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "babel-node",
    "testURL": "http://localhost:3003",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "radix": [
        "error",
        "as-needed"
      ],
      "no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "[rR]eact",
          "ignoreRestSiblings": true
        }
      ],
      "no-script-url": "off"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.31",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "glob": "^7.1.2",
    "image-extensions": "^1.1.0",
    "ncp": "^2.0.0",
    "raw-loader": "^0.5.1",
    "stream-to-promise": "^2.2.0"
  }
}
